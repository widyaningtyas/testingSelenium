pipeline {
    agent any
    stages {
        stage('Git Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Run test') {
            steps {

                withMaven(maven: 'maven-latest') {
                    sh 'mvn -v'
                    sh 'which mvn'
                    sh './mvnw clean verify'
                }
            }
        }
    }
}
